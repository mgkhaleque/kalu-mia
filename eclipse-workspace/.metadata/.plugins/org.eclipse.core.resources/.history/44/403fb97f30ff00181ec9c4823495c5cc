package loops;

public class UseForLoop {
/* walk up to 10 block>  walk<10;
 * walk 10 block> walk<=10;
 * walk incremental like> walk=walk+1 or walk++
 * walk faster pace> walk=walk+2 or walk+3
 * 
 * 
 * 
 * 
 * 
 * 
 *
 */
	public static void main(String[] args) {
		for (int walk=0; walk<10;walk = walk+1 ) {
			System.out.println(walk);
		}

	}

}	
