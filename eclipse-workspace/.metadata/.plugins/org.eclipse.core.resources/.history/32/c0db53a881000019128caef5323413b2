package nested;

public class NestedLoop {

	public static void main(String[] args) {
		System.out.println("Enter your choice for the following multiple questions:");
		for (int i=0; i<10; i++) {
			System.out.println(i);
			for (int j=0;j<i;j++) {
				System.out.print(" a");
				System.out.print(" b");
				System.out.print(" c");
				System.out.print(" d");
			}
			System.out.println();
		}
	}

}
