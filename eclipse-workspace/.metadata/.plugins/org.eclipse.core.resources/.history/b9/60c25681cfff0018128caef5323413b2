package exception;

public class ExceptionHandling {

	public static void main(String[] args) {
		
		int num1 = 10;
		
		try {
		int result = num1/0;
		System.out.println(result);
		}catch (Exception e) {
			System.out.println("you are not allowed to divide number by zero");
		}
		System.out.println("where is my result");

	}

}
